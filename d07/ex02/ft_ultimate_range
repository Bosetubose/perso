#define	abs(x)	((x) < 0 ? -(x) : (x))

int		ft_ultimate_range(int **range, int min, int max)
{
	int i;
	
	if (min >= max)
	{
		**range = NULL;
		min = max;
	}
	if (min < max)
	{
		**range = (int**)malloc(4 * (max - abs(min)));
		i = 0;
		while (i < min - max)
			range[0][i++] = min++;
	}
	return (min - max);
}
